{
  "name": "@easyauth-js/provider",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./google": {
      "types": "./dist/google.d.ts",
      "import": "./dist/google.js"
    },
    "./github": {
      "types": "./dist/github.d.ts",
      "import": "./dist/github.js"
    },
    "./credentials": {
      "types": "./dist/credentials.d.ts",
      "import": "./dist/credentials.js"
    },
    "./passkey": {
      "types": "./dist/passkey.d.ts",
      "import": "./dist/passkey.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "rimraf": "^5.0.5"
  }
}

